pipeline {
 agent any 
 stages {
     stage('read file') {
	    steps{
		  script{
			def  props = new Properties();
			File propFile = new File('C:/test/test.properties')
			props.load(propFile.newDataInputStream())
			def day1 = props[System.getenv("weekday")]
			println "day:${day1}"
		   }
		   }
	 }
	 }
	}
	   